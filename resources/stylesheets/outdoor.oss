     }
     [SIZE 6m <0.25mm:3px] WAY { color: lighten(@tertiaryColor,0.3); displayWidth: 0.25mm;}
   }
 }

 [MAG detail-] {
   [TYPE highway_road] {
     [SIZE 7m 0.1mm:3px<] {
       WAY#outline { color: @roadBColor; width: 7m; displayWidth: 0.5mm; priority: -1; joinCap: butt; zIndex: -1; }
       WAY { color: @roadColor; width: 7m;}
     }
     [SIZE 7m <0.1mm:3px] WAY { color: lighten(@roadColor,0.3); displayWidth: 0.1mm;}
   }

   [TYPE highway_unclassified,
