 }

 [MAG city-] {
   [TYPE highway_tertiary] {
     [SIZE 10m 0.45mm:3px<] {
       WAY#outline { color: darken(@tertiaryColor,0.6); width: 7m; displayWidth: 0.5mm; priority: -1; joinCap: butt;  zIndex: -1;}
       WAY { color: @tertiaryColor; width: 7m;}
     }
     [SIZE 7m <0.25mm:3px] WAY { color: lighten(@tertiaryColor,0.3); displayWidth: 0.25mm;}
