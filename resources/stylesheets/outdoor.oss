 }

 [MAG suburb-] {
   [TYPE railway_rail] {
     [SIZE 2m 0.25mm:3px<] {
       WAY#outline { color: #b3b3b3; width: 2m; displayWidth: 0.5mm; priority: -1; joinCap: butt; }
