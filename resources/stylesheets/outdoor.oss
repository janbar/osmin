  // Symbols
  COLOR amenitySymbolColor         = darken(@amenityColor, 0.4);
  COLOR busSymbolColor             = #7885b0;
  COLOR hospitalSymbolColor        = #da0092;
  COLOR peakSymbolColor            = #9c5e29;
  COLOR pharmacySymbolColor        = #da0092;
  COLOR postSymbolColor            = #e8f000;
  COLOR railwaySymbolColor         = @railwayColor;

SYMBOL embankment
  RECTANGLE 0,2.5 0.1 x 5 {
    AREA { color: #000000; }
  }
  RECTANGLE 2,1.5 0.1 x 3 {
    AREA { color: #000000; }
  }

SYMBOL small_embankment
  RECTANGLE 0,1 0.1 x 2 {
    AREA { color: #000000; }
  }
  RECTANGLE 2,0.5 0.1 x 1 {
    AREA { color: #000000; }
  }

SYMBOL border
  RECTANGLE 0,0.45 0.1 x 1 {
    AREA { color: #000000; }
  }
  RECTANGLE -0.45,0 1 x 0.1 {
    AREA { color: #000000; }
  }

SYMBOL cliff_small
  POLYGON 0,0 0.5,0 0.25,0.75 {
    AREA { color: lighten(@cliffColor, 0.5);}
  }

SYMBOL cliff
  POLYGON 0,0 1,0 0.5,1.5 {
    AREA { color: @cliffColor;}
  }

MODULE "include/symbols"
MODULE "include/route"
MODULE "include/track"
MODULE "include/waypoint"

STYLE
 // -------------------------------------------------------
 //
 // Synthetic
 //

 /*
 NODE "internal_start"
