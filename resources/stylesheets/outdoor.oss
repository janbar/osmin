     }
     [SIZE 2m <0.25mm:3px] WAY {color: #777777; dash: 30,20; joinCap: butt; gapColor: #ffffff; displayWidth: 0.25mm;}
   }

   [TYPE highway_motorway_link] {
     [SIZE 10m 0.45mm:3px<] {
