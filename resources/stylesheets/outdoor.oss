  IF cycloRoutes {
    [MAG suburb-] {
      [TYPE route_bicycle] ROUTE {color: @cyclewayColor; dash: 1,1; joinCap: butt; endCap: butt; offsetRel: sidecar; displayWidth: 0.4mm; priority: 50; }
      [TYPE route_mtb] ROUTE {color: @cyclewayColor; preferColorFeature: true; dash: 6,3; joinCap: butt; endCap: butt; offsetRel: sidecar; displayWidth: 0.2mm; priority: 50; }
    }
    [MAG close-] {
      [TYPE route_bicycle, route_mtb] ROUTE.TEXT {label: Ref.name; color: @cyclewayColor; size: 1.0; displayOffset: 3.0mm; priority: 19;}
    }
    [MAG veryClose-] {
      [TYPE route_bicycle] ROUTE {displayWidth: 0.8mm; }
      [TYPE route_mtb] ROUTE {displayWidth: 0.4mm; }
    }
  }
  IF hikingRoutes  {
    [MAG suburb-] {
      [TYPE route_hiking] ROUTE {color: #ff0000; preferColorFeature: true; offsetRel: sidecar; displayWidth: 0.4mm; priority: 50; }
    }
    [MAG veryClose-] {
      [TYPE route_hiking] ROUTE {displayWidth: 0.8mm; }
    }
  }

 [MAG block-] {
   // Cycleways on way
   [FEATURE Sideway.cyclewayLaneLeft] {
     WAY#cyclewayLaneLeft {color: @cyclewayColor; dash: 1,1; joinCap: butt; endCap: butt; displayWidth: 0.4mm; offsetRel: leftOutline; displayOffset: 1mm; priority: 1;}
