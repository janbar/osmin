 // -------------------------------------------------------
 //
 // natural_* (without coastline)
 //

 [MAG county-] {
   [TYPE natural_glacier] AREA { color: @glacierColor; }
   [TYPE natural_bare_rock] AREA { color: @rockColor; }
   [TYPE natural_fell] AREA { color: @fellColor; }
   [TYPE natural_heath] AREA { color: @heathColor; }
   [TYPE natural_grassland] AREA { color: @grassColor; }
   [TYPE wood] AREA { color: @woodColor; }
   [TYPE natural_wetland_marsh] AREA { color: @wetlandMarshColor;pattern: "wetland_bog";patternMinMag: cityOver;}
   [TYPE natural_wetland, natural_wetland_tidalflat] AREA { color: @wetlandColor;pattern: "wetland"; patternMinMag: cityOver;}
   [TYPE natural_water] AREA { color: @waterColor; }
   [TYPE natural_beach] AREA { color: @beachColor; }
   [TYPE natural_land] AREA { color: @landColor; }
   [TYPE natural_scrub] AREA { color: @scrubColor; pattern: "scrub"; patternMinMag: cityOver; }
   [TYPE natural_sand] AREA { color: @sandColor; }
 }

 [MAG suburb-] {
   [TYPE natural_peak] NODE.ICON { symbol: natural_peak; }
  }

 [MAG detail-] {
   [TYPE natural_glacier,
         natural_fell,
         natural_heath,
         wood,
         natural_scrub,
         natural_sand,
         natural_wetland,
         natural_water,
         natural_beach,
         natural_land]
     AREA.TEXT { label: Name.name; }

   [TYPE natural_peak] {
     NODE.TEXT#peakName { label: Name.name; style: emphasize; color: @peakLabelColor; priority: @labelPrioPeak; size: 0.9; position: 1; }
     NODE.TEXT#ele { label: Ele.inLocaleUnit; style: emphasize; color: @peakLabelColor; priority: @labelPrioPeak; size: 0.9; position: 2;}
   }

  [TYPE natural_cave_entrance] {
    NODE.ICON { name: cave; }
    NODE.TEXT { label: Name.name; size: 1.0; }
  }

 }

  [MAG detail-close] {
    [TYPE natural_cliff] {
      WAY {color: lighten(@cliffColor,0.5); displayWidth: 0.10mm; }
      WAY.SYMBOL { symbol: cliff_small; displayOffset: 0.8mm; symbolSpace: 6mm;}
    }
  }
  [MAG closer-] {
    [TYPE natural_cliff] {
      WAY {color: @cliffColor; displayWidth: 0.13mm; }
      WAY.SYMBOL { symbol: cliff; displayOffset: 1.5mm;}
    }
    [TYPE natural_spring] {
      NODE.ICON { symbol: natural_spring; }
      NODE.TEXT { label: Name.name; color: @waterLabelColor; priority: @labelPrioSpring; size: 0.9; }
    }
  }

 // -------------------------------------------------------
 //
 // elevation_contour_*
 //

 [MAG detail-] {
   [TYPE elevation_contour_major] {
     WAY {color: @majorContourColor; displayWidth: 0.13mm; }
   }

   [TYPE elevation_contour_medium] {
     WAY {color: @mediumContourColor; displayWidth: 0.11mm; }
   }

   [TYPE elevation_contour_minor] {
     WAY {color: @minorContourColor; displayWidth: 0.075mm; }
   }
 }

 [MAG 15-] {
   [TYPE elevation_contour_major] {
     WAY.TEXT { label: Ele.inLocaleUnit; color: @majorContourColor; displayOffset: -1.2mm; size: 0.7;}
   }
   [TYPE elevation_contour_medium] {
     WAY.TEXT { label: Ele.inLocaleUnit; color: @mediumContourColor; displayOffset: -1.2mm; size: 0.7;}
   }
 }

 [MAG 19-] {
   [TYPE elevation_contour_minor] {
     WAY.TEXT { label: Ele.inLocaleUnit; color: @minorContourColor;  displayOffset: -1.2mm; size: 0.7;}
   }
 }

 // -------------------------------------------------------
 //
 // waterway_*
 //

 [MAG stateOver-] {
   [TYPE waterway_river] {
     [SIZE 10m 0.25mm:3px<] WAY {color: @waterColor; width: 10m;}
     [SIZE 10m <0.25mm:3px] WAY {color: @waterColor; displayWidth: 0.25mm; }
   }

  [TYPE waterway_riverbank] {
     AREA { color: @waterColor; }
   }
 }

 [MAG proximity-] {

   [TYPE waterway_canal] {
     [FEATURE Tunnel SIZE 5m 0.25mm:3px<] {
       WAY#outline { dash: 0.5,0.5; }
       WAY { color: lighten(@waterColor,0.3);}
     }
     [SIZE 5m 0.25mm:3px<] WAY {color: @waterColor; width: 5m;}
     [SIZE 5m <0.25mm:3px] WAY {color: @waterColor; displayWidth: 0.25mm; }
   }

   [TYPE waterway_dock] {
     AREA { color: @waterColor; }
     AREA.TEXT { label: Name.name; }
   }

   [TYPE waterway_river,
         waterway_canal] {
     WAY.TEXT { label: Name.name;}
   }

 }

 [MAG suburb-] {
   [TYPE waterway_stream] {
     [SIZE 2m 0.15mm:3px<] WAY {color: @waterColor; width: 2m;}
     [SIZE 2m <0.15mm:3px] WAY {color: @waterColor; displayWidth: 0.15mm;}

     [FEATURE Tunnel] {
       WAY { dash: 0.4,4;}
     }
   }

   [TYPE waterway_drain, waterway_ditch] {
     [SIZE 3m 0.1mm:3px<] WAY {color: @waterColor; width: 3m;}
     [SIZE 3m <0.1mm:3px] WAY {color: @waterColor; displayWidth: 0.1mm; }
   }

   [TYPE waterway_weir] {
     WAY {color: lighten(@waterColor, 0.3); width: 3m; dash: 0.5,2;}

     AREA { color: lighten(@waterColor, 0.3);}

     NODE.ICON { symbol: waterway_weir;}
   }
   [TYPE waterway_waterfall] {
     WAY {color: lighten(@waterColor, 0.3); width: 3m; dash: 0.5,2;}

     AREA { color: lighten(@waterColor, 0.3);}

     NODE.ICON { symbol: waterway_waterfall;}
    }
 }

 [MAG block-] {
   [TYPE waterway_stream,
       waterway_weir, waterway_waterfall,
       waterway_drain, waterway_ditch]
     WAY.TEXT { label: Name.name; color: @waterLabelColor; priority: @labelPrioNatural;}

  [TYPE waterway_weir,
        waterway_waterfall]
    NODE.TEXT { label: Name.name; color: @waterLabelColor; priority: @labelPrioNatural; }
 }

 // -------------------------------------------------------
 //
 // highway_*, railway_*, route_ferry
 //

 [MAG continent-] {
   [TYPE highway_motorway] {
     [SIZE 10m 0.45mm:3px<] {
       WAY#outline { color: @motorwayBColor; width: 10m; displayWidth: 1.5mm; priority: -1; joinCap: butt; zIndex: -1; }
       WAY { color: @motorwayColor; width: 7m;}
     }
     [SIZE 10m <0.45mm:3px] WAY { color: @motorwayBColor; displayWidth: 0.45mm;}
   }
 }

 [MAG state-] {
   [TYPE highway_trunk,highway_motorway_trunk] {
     [SIZE 10m 0.45mm:3px<] {
       WAY#outline { color: darken(@trunkColor,0.4); width: 12m; displayWidth: 0.5mm; priority: -1; joinCap: butt; zIndex: -1;}
       WAY { color: @trunkColor; width: 12m;}
     }
     [SIZE 10m <0.45mm:3px] WAY { color: lighten(@trunkColor,0.3); displayWidth: 0.4mm;}
   }
 }

 [MAG county-] {
   [TYPE highway_primary, highway_motorway_primary] {
     [SIZE 10m 0.4mm:3px<] {
       WAY#outline { color: darken(@primaryColor,0.4); width: 10m; displayWidth: 0.5mm; priority: -1; joinCap: butt; zIndex: -1; }
       WAY { color: @primaryColor; width: 10m;}
     }
     [SIZE 10m <0.4mm:3px] WAY { color: lighten(@primaryColor,0.3); displayWidth: 0.4mm;}
   }
 }

 [MAG proximity-] {
   [TYPE highway_secondary] {
     [SIZE 10m 0.45mm:3px<] {
       WAY#outline { color: darken(@secondaryColor,0.4); width: 8m; displayWidth: 0.5mm; priority: -1; joinCap: butt;  zIndex: -1;}
       WAY { color: @secondaryColor; width: 8m;}
     }
     [SIZE 8m <0.25mm:3px] WAY { color: #fdd654; displayWidth: 0.25mm; }
